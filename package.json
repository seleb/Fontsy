{
	"name": "fontsy",
	"version": "2.1.0",
	"description": "bitsy font tool",
	"main": "./src/index.js",
	"author": "Sean S. LeBlanc",
	"scripts": {
		"start": "parcel index.html",
		"build": "npm run build:cleanup && npm run build:parcel",
		"build:cleanup": "rm -rf .cache && rm -rf dist",
		"build:parcel": "parcel build index.html -d dist --no-source-maps --public-url ./",
		"prettier": "prettier \"src/**/*.js?(x)\" --list-different"
	},
	"devDependencies": {
		"@babel/core": "^7.14.2",
		"autoprefixer": "^9.0.0",
		"parcel": "1.12.3",
		"parcel-bundler": "^1.12.5",
		"prettier": "^2.3.0"
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"draw-1-bit": "^1.0.3",
		"just-give-me-the-pixels": "^1.0.2",
		"preact": "^10.5.13",
		"react-redux": "^7.2.4",
		"redux": "^4.1.0",
		"redux-devtools-extension": "^2.13.9"
	},
	"alias": {
		"react": "preact/compat",
		"react-dom": "preact/compat"
	},
	"prettier": {
		"useTabs": true,
		"singleQuote": true,
		"printWidth": 160
	}
}
